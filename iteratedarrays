for (var i = 0; i < enemyStringArray.length; i++ )

var numbersAsStrings = ["one", "two", "three"];
for (var i = 0; i < numbersAsStrings.length; i++)

var nutsAlmonds = ["nuts", "almonds"];
for (var i = 0; i < nutsAlmonds.length; i++);

